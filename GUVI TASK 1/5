import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int f,i,j;
		System.out.println("enter the no rows");
		f=sc.nextInt();
		for(i=f;i>=1;i--)
		{
			for(j=f;j>=1;j--)
			{
				System.out.print(i>j?i:j);
				
			}
			System.out.println();
		}
		

	}

}
